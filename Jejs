#include <iostream>
using namespace std;

int main() {
    int x, y, z, w;
    cout << "Podaj cztery liczby całkowite: ";
    cin >> x >> y >> z >> w;

    if (x == y && y == z && z == w) {
        cout << "Kareta" << endl;
    } else if ((x == y && z == w) || (x == z && y == w) || (x == w && y == z)) {
        cout << "dwie pary" << endl;
    } else if ((x == y && y == z) || (x == y && y == w) || (x == z && z == w)) {
        cout << "trójka" << endl;
    } else if (x == y || x == z || x == w || y == z || y == w || z == w) {
        cout << "para" << endl;
    } else {
        cout << "nic" << endl;
    }

    return 0;
}
